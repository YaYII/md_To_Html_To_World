{
  "name": "markdown-to-word",
  "displayName": "Markdown to Word",
  "description": "将Markdown文档转换为Word文档:作者杨英：抖音：木易君，抖音号：YI.11.13",
  "version": "0.1.8",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/markdown-to-word.git"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onLanguage:markdown",
    "onCommand:markdowntoword.markdown-to-word.convert",
    "onCommand:markdowntoword.markdown-to-word.diagnostics",
    "onCommand:markdowntoword.markdown-to-word.checkEnvironment",
    "onCommand:markdowntoword.markdown-to-word.installDependencies"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "markdowntoword.markdown-to-word.convert",
        "title": "配置world文档",
        "category": "Markdown"
      },
      {
        "command": "markdowntoword.markdown-to-word.convertDirect",
        "title": "生成Word文档",
        "category": "Markdown"
      },
      {
        "command": "markdowntoword.markdown-to-word.convertToHtml",
        "title": "生成HTML文档",
        "category": "Markdown"
      },
      {
        "command": "markdowntoword.markdown-to-word.batchConvertToWord",
        "title": "批量生成Word文档",
        "category": "Markdown"
      },
      {
        "command": "markdowntoword.markdown-to-word.batchConvertToHtml",
        "title": "批量生成HTML文档",
        "category": "Markdown"
      },
      {
        "command": "markdowntoword.markdown-to-word.diagnostics",
        "title": "环境诊断",
        "category": "Markdown to Word"
      },
      {
        "command": "markdowntoword.markdown-to-word.checkEnvironment",
        "title": "检查环境",
        "category": "Markdown to Word"
      },
      {
        "command": "markdowntoword.markdown-to-word.installDependencies",
        "title": "安装依赖",
        "category": "Markdown to Word"
      },
      {
        "command": "markdowntoword.markdown-to-word.test",
        "title": "测试命令",
        "category": "Markdown to Word"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "resourceExtname == .md",
          "command": "markdowntoword.markdown-to-word.convert",
          "group": "navigation"
        },
        {
          "when": "resourceExtname == .md",
          "command": "markdowntoword.markdown-to-word.convertDirect",
          "group": "navigation"
        },
        {
          "when": "resourceExtname == .md",
          "command": "markdowntoword.markdown-to-word.convertToHtml",
          "group": "navigation"
        },
        {
          "when": "explorerResourceIsFolder",
          "command": "markdowntoword.markdown-to-word.batchConvertToWord",
          "group": "navigation"
        },
        {
          "when": "explorerResourceIsFolder",
          "command": "markdowntoword.markdown-to-word.batchConvertToHtml",
          "group": "navigation"
        }
      ],
      "editor/context": [
        {
          "when": "resourceExtname == .md",
          "command": "markdowntoword.markdown-to-word.convert",
          "group": "navigation"
        },
        {
          "when": "resourceExtname == .md",
          "command": "markdowntoword.markdown-to-word.convertDirect",
          "group": "navigation"
        },
        {
          "when": "resourceExtname == .md",
          "command": "markdowntoword.markdown-to-word.convertToHtml",
          "group": "navigation"
        }
      ],
      "commandPalette": [
        {
          "command": "markdowntoword.markdown-to-word.convert",
          "when": "resourceExtname == .md"
        },
        {
          "command": "markdowntoword.markdown-to-word.convertDirect",
          "when": "resourceExtname == .md"
        },
        {
          "command": "markdowntoword.markdown-to-word.convertToHtml",
          "when": "resourceExtname == .md"
        },
        {
          "command": "markdowntoword.markdown-to-word.batchConvertToWord",
          "when": "explorerViewletVisible"
        },
        {
          "command": "markdowntoword.markdown-to-word.batchConvertToHtml",
          "when": "explorerViewletVisible"
        },
        {
          "command": "markdowntoword.markdown-to-word.diagnostics"
        },
        {
          "command": "markdowntoword.markdown-to-word.checkEnvironment"
        },
        {
          "command": "markdowntoword.markdown-to-word.installDependencies"
        },
        {
          "command": "markdowntoword.markdown-to-word.test"
        }
      ]
    },
    "configuration": {
      "title": "Markdown to Word",
      "properties": {
        "markdown-to-word.pythonPath": {
          "type": "string",
          "default": "",
          "description": "Python解释器路径（可选）"
        },
        "markdown-to-word.useVirtualEnv": {
          "type": "boolean",
          "default": true,
          "description": "是否使用虚拟环境"
        },
        "markdown-to-word.defaultOutputDirectory": {
          "type": "string",
          "default": "",
          "description": "默认输出目录"
        },
        "markdown-to-word.defaultPageSize": {
          "type": "string",
          "enum": [
            "A4",
            "Letter",
            "Legal"
          ],
          "default": "A4",
          "description": "默认页面大小"
        },
        "markdown-to-word.defaultOrientation": {
          "type": "string",
          "enum": [
            "portrait",
            "landscape"
          ],
          "default": "portrait",
          "description": "默认页面方向"
        },
        "markdown-to-word.defaultFontFamily": {
          "type": "string",
          "default": "微软雅黑",
          "description": "默认字体"
        },
        "markdown-to-word.defaultFontSize": {
          "type": "number",
          "default": 12,
          "description": "默认字号"
        },
        "markdown-to-word.defaultLineSpacing": {
          "type": "number",
          "default": 1.5,
          "description": "默认行间距"
        },
        "markdown-to-word.includeToc": {
          "type": "boolean",
          "default": true,
          "description": "是否包含目录"
        },
        "markdown-to-word.tocDepth": {
          "type": "number",
          "default": 3,
          "minimum": 1,
          "maximum": 6,
          "description": "目录深度"
        },
        "markdown-to-word.preserveImages": {
          "type": "boolean",
          "default": true,
          "description": "是否保留图片"
        },
        "markdown-to-word.imageMaxWidth": {
          "type": "number",
          "default": 800,
          "description": "图片最大宽度（像素）"
        },
        "markdown-to-word.autoInstallDependencies": {
          "type": "boolean",
          "default": true,
          "description": "自动安装缺失的Python依赖"
        },
        "markdown-to-word.showDiagnosticInfo": {
          "type": "boolean",
          "default": false,
          "description": "在转换过程中显示诊断信息"
        },
        "markdown-to-word.markdownToWordUserConfig": {
          "type": "string",
          "default": "",
          "description": "用户配置数据（自动管理，请勿手动修改）"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "update-version": "node ./scripts/update-version.js",
    "package": "npm run update-version && npx @vscode/vsce package"
  },
  "dependencies": {
    "js-yaml": "^4.1.0"
  },
  "publisher": "markdowntoword",
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/js-yaml": "^4.0.5",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.37",
    "@types/vscode": "^1.60.0",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.2.4",
    "vscode-test": "^1.5.2"
  }
}
